         id="default"
         name="Default Configuration"
         doc="templateDocs/DefaultConfig.xml"
         paths="templates/cnfs/default"
         priority="15"
         managed="ext/.*,plugins/.*,build-template.xml,build.properties"
         ignores="/cache/"
         version="0"
         category="AAA/Core"
      />
   </extension>

</plugin>