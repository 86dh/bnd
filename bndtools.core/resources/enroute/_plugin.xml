	<!--
	
		ENROUTE TEMPLATES
	-->
	
	<extension point="bndtools.core.cnfTemplates">
      <template
         id="enRoute"
         name="OSGi enRoute Configuration (Alpha)"
         doc="${enroute}/doc.xml"
         paths="${enroute}/cnf"
         priority="0"
         managed="ext/.*"
         ignores="/cache/"
         version="10"
         category="AAA/Core"
         nobuild="true"
         icon="enroute/enroute-bw-16x16.png"
      />
   </extension>
	

	<extension point="bndtools.core.projectTemplates">
		<template name="OSGi enRoute "
			priority="400" category="AAA/Core"
			class="org.bndtools.core.templates.enroute.ProjectTemplate" 
			doc="enroute/project-template.xml"
			icon="enroute/enroute-bw-16x16.png" />
	</extension>





















