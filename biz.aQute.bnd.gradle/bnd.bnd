-dependson: biz.aQute.bnd

Private-Package: \
    aQute.bnd.gradle.*

-digests: MD5, SHA1

Bundle-Description: The bnd gradle plugin.

-buildpath: \
  ee.j2se;version=${javac.ee}, \
  aQute.libg;version=latest, \
  biz.aQute.bndlib;version=latest

-includeresource: \
  OSGI-OPT/src=src, \
  resources, \
  @${repo;biz.aQute.bndlib;latest}!/!META-INF/*, \
  @${repo;biz.aQute.bnd;latest}!/embedded-repo.jar
